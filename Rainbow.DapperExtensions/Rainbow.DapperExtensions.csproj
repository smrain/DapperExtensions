<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>1.0.0</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Description>基于Dapper-Extensions扩展的ORM框架，支持异步CRUD操作、Lambda表达式条件查询、字段部分查询与部分更新等功能。</Description>
    <Authors>Frank Yu</Authors>
    <PackageTags>Dapper ORM Lambda</PackageTags>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageIcon>rainbow_1f308.png</PackageIcon>
    <PackageReleaseNotes>https://www.apache.org/licenses/LICENSE-2.0</PackageReleaseNotes>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.78" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\rainbow_1f308.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
